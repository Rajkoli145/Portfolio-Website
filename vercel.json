{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "admin/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/admin",
            "dest": "/admin/index.html"
        },
        {
            "src": "/test",
            "dest": "/api/index.js",
            "methods": ["GET"]
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js",
            "methods": ["GET", "POST", "OPTIONS"]
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "public": true
}
