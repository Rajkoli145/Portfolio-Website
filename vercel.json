{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/test",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}
